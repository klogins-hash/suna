# Northflank Environment Variables Template
ENV_MODE=production
DISABLE_AUTH=true

# Frontend
NEXT_PUBLIC_DISABLE_AUTH=true
NEXT_PUBLIC_API_URL=https://your-backend-url.northflank.app

# Redis (will be provided by Northflank Redis addon)
REDIS_HOST=redis-host
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_SSL=false

# Required API Keys (add your actual keys)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
MODEL_TO_USE=claude-3-5-sonnet-20241022

# Security (generate random strings)
API_KEY_SECRET=your_random_secret_key_here
MCP_CREDENTIAL_ENCRYPTION_KEY=your_encryption_key_here

# Optional services (can be left empty for basic deployment)
TAVILY_API_KEY=
FIRECRAWL_API_KEY=
DAYTONA_API_KEY=
DAYTONA_SERVER_URL=
DAYTONA_TARGET=

PORT=8000
